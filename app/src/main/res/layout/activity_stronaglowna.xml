<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".nologin">

    <ImageButton
        android:id="@+id/stworzPostac"
        android:layout_width="117dp"
        android:layout_height="152dp"
        android:layout_marginTop="112dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/stworz_postac117x96"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="204dp"
        android:layout_height="226dp"
        android:layout_marginTop="40dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stworzPostac"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/losowanie"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/nickclick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Błąd"
        android:textSize="50dp"
        app:layout_constraintBottom_toTopOf="@+id/stworzPostac"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="117dp"
        android:layout_height="157dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.724"
        app:layout_constraintStart_toEndOf="@+id/stworzPostac"
        app:layout_constraintTop_toBottomOf="@+id/nickclick"
        app:layout_constraintVertical_bias="0.084"
        app:srcCompat="@drawable/twoje_postacie117x152" />
</androidx.constraintlayout.widget.ConstraintLayout>